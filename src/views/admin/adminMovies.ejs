<%- include('../partials/head') %>

<body class="body-admin-movie">
    <div class="all-section-movie">
        <section class="statistic__movies">
            <article class="table__statistic-movies">
                <div class="info__statistic-movies">
                    <h2 class="title__statistic-movies">Informacion de peliculas</h2>
                    <div class="dashboard-movies">
                        <div class="dashboard-info-movies"></div>
                        <div class="information">
                            <span class="info__movies amount">Total de peliculas :</span>
                            <span class="info__movies spain">Peliculas en español :</span>
                            <span class="info__movies english">Peliculas en ingles :</span>
                            <span class="info__movies languages">Peliculas en otro idioma :</span>
                        </div>
                    </div>
                </div>
            </article>
        </section>
        <section class="statistic__movies">
            <article class="table__statistic-movies">
                <div class="info__statistic-movies">
                    <h2 class="title__statistic-movies">Peliculas compradas</h2>
                    <div class="dashboard-movies">
                        <div class="dashboard-info-movies"></div>
                        <div class="information">
                            <span class="info__movies bought">Total de peliculas :</span>
                            <span class="info__movies bought-spain">Peliculas en español :</span>
                            <span class="info__movies bought-english">Peliculas en ingles :</span>
                            <span class="info__movies bought-languages">Peliculas en otro idioma :</span>
                        </div>
                    </div>
                </div>
            </article>
        </section>
        <section class="statistic__movies">
            <article class="table__statistic-movies">
                <div class="info__statistic-movies">
                    <h2 class="title__statistic-movies">Peliculas alquiladas</h2>
                    <div class="dashboard-movies">
                        <div class="dashboard-info-movies"></div>
                        <div class="information">
                            <span class="info__movies rented-amount">Total de peliculas :</span>
                            <span class="info__movies rented-spain">Peliculas en español :</span>
                            <span class="info__movies rented-english">Peliculas en ingles :</span>
                            <span class="info__movies rented-languages">Peliculas en otro idioma :</span>
                        </div>
                    </div>
                </div>
            </article>
        </section>
        <section class="statistic__movies">
            <article class="addition-board">
                <a href="#">
                    <span class="addition-delete">agregar o eliminar peliculas
                        <i class="fas fa-plus-circle"></i>
                    </span>
                </a>
            </article>
        </section>
    </div>
    <%- include('../partials/adminFooter') %>
</body>
</html>